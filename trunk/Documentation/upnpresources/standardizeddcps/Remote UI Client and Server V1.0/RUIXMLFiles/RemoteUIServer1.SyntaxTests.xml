<ServiceControlSyntaxTestCases>
	<ServiceType>RemoteUIServer</ServiceType>
	<ServiceVersion>1</ServiceVersion>
	<TestCaseList>
		<TestCase>
			<Id>1</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>GetCompatibleUIs</ActionName>
			<InArgs>
				<InputDeviceProfile>&lt;deviceprofile xmlns="urn:schemas-upnp-org:remoteui:devprofile-1-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema"&gt;
   &lt;protocol shortName="XYZ"/&gt;
   &lt;protocol shortName="QRS"/&gt;
&lt;/deviceprofile&gt;</InputDeviceProfile>
				<UIFilter>Some Filter String</UIFilter>
			</InArgs>
			<ExpectedReturnCode>ACTION_AND_INARGS_ARE_VALID</ExpectedReturnCode>
		</TestCase>
		<TestCase>
			<Id>2</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>SetUILifetime</ActionName>
			<InArgs>
				<UI>XYZ://1.23.345.3/Super_Chess</UI>
				<Lifetime>11</Lifetime>
			</InArgs>
			<ExpectedReturnCode>ACTION_AND_INARGS_ARE_VALID</ExpectedReturnCode>
		</TestCase>
	</TestCaseList>
</ServiceControlSyntaxTestCases>
