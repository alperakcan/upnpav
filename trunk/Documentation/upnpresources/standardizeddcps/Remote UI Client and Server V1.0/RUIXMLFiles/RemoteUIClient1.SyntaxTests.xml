<ServiceControlSyntaxTestCases>
	<ServiceType>RemoteUIClient</ServiceType>
	<ServiceVersion>1</ServiceVersion>
	<TestCaseList>
		<TestCase>
			<Id>1</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>Connect</ActionName>
			<InArgs>
			  <RequestedConnections>5604,
XRT://1.23.345.1/My_Music_Player0xa12c67
local://127.4.6.1/null,
VNC://1.23.345.2/My_Photo_Viewer,
RDP://1.23.345.3/Super_Chess
</RequestedConnections>
			</InArgs>
			<ExpectedReturnCode>ACTION_AND_INARGS_ARE_VALID</ExpectedReturnCode>
		</TestCase>
		<TestCase>
			<Id>2</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>Disconnect</ActionName>
			<InArgs>
			  <RequestedDisconnects>XRT://1.23.345.1/My_Music_Player0xa12c67</RequestedDisconnects>
			</InArgs>
