			<Id>7</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>RemoveUIListing</ActionName>
			<InArgs>
			  <RemoveUIList>VNC://1.23.345.2/My_Photo_Viewer,RDP://1.23.345.3/Super_Chess</RemoveUIList>
