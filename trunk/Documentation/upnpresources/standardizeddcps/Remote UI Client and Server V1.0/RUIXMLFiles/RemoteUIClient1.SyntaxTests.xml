			<Id>6</Id>
			<Category>Valid Action And Valid InArgs</Category>
			<ActionName>AddUIListing</ActionName>
			<InArgs>
			  <InputUIList>&lt;uilist xmlns="urn:schemas-upnp-org:remoteui:uilist-1-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"&gt;
  &lt;ui&gt;
    &lt;uiID&gt;6789-568&lt;/uiID&gt;
    &lt;name&gt;DVD Browser&lt;/name&gt;
    &lt;protocol shortName="VNC"&gt;
      &lt;uri&gt;VNC://1.8.7.2:5920&lt;/uri&gt;
    &lt;/protocol&gt;
  &lt;/ui&gt;
&lt;/uilist&gt;
</InputUIList>
			</InArgs>
			<ExpectedReturnCode>ACTION_AND_INARGS_ARE_VALID</ExpectedReturnCode>
		</TestCase>
